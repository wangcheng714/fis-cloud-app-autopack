{%style id="/widget/header/header.inline.less"%}/* @fileoverview 通用头样式 */
.common-widget-header {
  /* 搜索框外框 start */

  /* 搜索框外框 end */

  /* 搜索框内框 start */

}
.common-widget-header .nav {
  position: relative;
  height: 44px;
  line-height: 44px;
  background-color: #36393c;
  background: -webkit-gradient(linear, 0 100%, 0 0, from(#36393c), to(#585c63));
  border-bottom: 1px solid #222222;
  text-align: center;
}
.common-widget-header .side-l {
  position: absolute;
  top: 6px;
  left: 8px;
  display: inline-block;
  height: 30px;
  width: 91px;
  line-height: 30px;
  text-align: center;
  color: #bbbcbd;
  border: 1px solid #2c2e31;
  background-color: #434951;
  border-radius: 2px;
}
.common-widget-header .btn-l {
  display: inline-block;
  float: left;
  width: 47px;
  font-size: 15px;
  border-right: 1px solid #2c2e31;
}
.common-widget-header .btn-l a {
  display: inline-block;
  height: 100%;
  width: 100%;
  color: #dcdcdc;
  background-color: #434951;
}
.common-widget-header .btn-l a:active {
  background-color: #363a41;
}
.common-widget-header .btn-r {
  display: inline-block;
  float: right;
  width: 43px;
}
.common-widget-header .btn-r a {
  display: inline-block;
  height: 100%;
  width: 100%;
  background: #434951 url(/static/third/images/prod_fe5d94c.png) no-repeat 14px;
  /*background: url("data:image/png;base64,") no-repeat;*/

  background-size: 16px 16px;
  /*background-position: 14px;*/

  /*background-color:#434951;*/

}
.common-widget-header .btn-r a:active {
  background-color: #363a41;
}
.common-widget-header .btn-r .top {
  height: 7px;
  margin-bottom: 1px;
  /*overflow: hidden;*/

}
.common-widget-header .btn-r .bottom {
  height: 7px;
}
.common-widget-header .btn-r b {
  display: inline-block;
  height: 5px;
  width: 5px;
  border: 1px solid #d8d8d8;
  background-color: #d8d8d8;
}
.common-widget-header .btn-r .blank {
  background-color: #40464d;
}
.common-widget-header .btn-r .sp {
  margin-left: 1px;
}
.common-widget-header .more-icon {
  display: inline-block;
  width: 43px;
  height: 30px;
  border-left: 1px solid #2c2e31;
  background-position: 14px 7px;
}
.common-widget-header .title {
  display: inline-block;
  color: #f4f4f4;
  text-align: center;
  width: 100px;
  /*width:100%;*/

}
.common-widget-header .title .txt {
  font-size: 18px;
}
.common-widget-header .hb-wrapper {
  position: absolute;
  right: 5px;
  top: 0px;
  width: 22px;
  height: 44px;
  padding-right: 10px;
}
.common-widget-header .sb-wrapper {
  position: absolute;
  right: 52px;
  top: 0px;
  width: 20px;
  height: 44px;
}
.common-widget-header .index-loc-pic {
  background: url(/static/third/images/index_loc_1801942.png) no-repeat 0 0;
  background-size: 24px 130px;
}
.common-widget-header .home-btn {
  /*position:absolute;*/

  margin-top: 12px;
  display: inline-block;
  height: 20px;
  width: 22px;
  /*right:14px;*/

  /*top:12px;*/

  background-position: 0px -111px;
}
.common-widget-header .srh-btn {
  /*position:absolute;*/

  display: inline-block;
  margin-top: 12px;
  height: 20px;
  width: 22px;
  /*right:52px;*/

  /*top:12px;*/

  background-position: 0px -89px;
}
.common-widget-header .se-wrap {
  padding: 8px 8px;
  /*border-bottom: 1px solid #cecece;*/

  background-color: #ededed;
  position: relative;
}
.common-widget-header .se-wrap .se-poi-form {
  height: 36px;
  background-color: #ffffff;
  position: relative;
}
.common-widget-header .se-wrap .btn-clear {
  position: absolute;
  right: 0.286em;
  top: 50%;
  margin-top: -0.714em;
  width: 1.464em;
  height: 1.464em;
  display: none;
  background-position: 0 -5.964em;
  z-index: 100000;
}
.common-widget-header .se-wrap input {
  width: 100%;
  height: 100%;
  /*padding-left:0.25em;*/

  color: #333333;
  border: none;
  background: none;
}
.common-widget-header .se-wrap input.geo {
  color: #4C78CE;
}
.common-widget-header .se-inner {
  height: 32px;
  padding-left: 26px;
  position: relative;
  -webkit-box-shadow: inset 1px 1px 2px #dddddd;
  background: #fff;
  border: 1px solid #9a9a9a;
  width: auto;
  left: 0;
  right: 0;
}
.common-widget-header .se-poi-form input {
  padding: 5px;
  height: 27px;
  /*line-height: 27px;*/

}
.common-widget-header .se-poi table {
  border: 1px solid #9a9a9a;
  background: #fff;
}
.common-widget-header .se-btn {
  height: 36px;
  width: 49px;
  border-left: 1px solid #9a9a9a;
  background: -webkit-gradient(linear, 0 100%, 0 0, from(#e7e7e7), to(#f5f5f5));
}
.common-widget-header .se-btn b {
  display: inline-block;
  width: 23px;
  height: 23px;
  margin-top: 6px;
  margin-left: 14px;
  background: url(/static/third/images/search_ico_0190136.png) no-repeat;
  background-size: 23px 23px;
}
.common-widget-header .hide {
  display: none;
}
{%/style%}<!-- @fileOverview 头部模板 -->
<header class="common-widget-header">
    <div class="nav">
        <div class="side-l clearfix">
            <span class="btn-l"><a href="{%$commonUrl.header.wise%}" target="_blank">百度</a></span>
            <span class="btn-r"><a href="{%$commonUrl.header.switch_product%}">&nbsp;</a></span>
        </div>
        <div class="title"><span class="txt">百度地图</span></div>
        {%if $hideRight != true%}
            <div class="side-r">
                <a jsaction="jump" data-log="{code:'{%$COM_STAT_CODE.HEADER_BACKTO_INDEX%}'}" href="/mobile/webapp/index/index" class="hb-wrapper">
                    <span class="index-loc-pic home-btn"></span>
                </a>
                <a href="javascript:void(0)" class="sb-wrapper">
                    <span class="index-loc-pic srh-btn"></span>
                </a>
            </div>
        {%/if%}
    </div>
</header>
