{%/style%}<!-- @fileoverview 外卖列表模板 --><div class="place-widget-taskoutlist">    <div id="rl_16_wrapper" class="rl_wrapper tk_rl_list" style="display: block;">        <ul id="rl_16_list" class="rl_list poi-list">            {%foreach $data.shoplist as $shop%}            <li class="takeout-list" data-index={%$shop@index%}>                {%if isset($shop.detailUrl) && $shop.dishCount != 0 %}					<a href="{%$shop.detailUrl%}">				{%else if ($data.platform == 'android')%}					<a href="javascript:void(0)" class="tj" onclick="popupDialog('{%$shop.telphone%}')">				{%else%}					<a class="tj" href="tel:{%$shop.telphone%}">                {%/if%}                    <h4>                        <strong class="rl_li_title">                            {%if isset($shop.shopName)%}                                {%$shop.shopName%}                            {%else%}                                {%$shop.brandName%}                            {%/if%}                        </strong>						<span class="rl_li_distanceXY">							{%if isset($shop.distanceXY)%}                                {%round($shop.distanceXY)%}m                            {%/if%}						</span>                    </h4>                    <p class="fixforempty fixformargin_star">						<span class="rl_li_score sm star_wrapper">							<span class="star_box">								<span class="star_scroe" style="width:{%$shop.score*15%}px"></span>							</span>						</span>                        <span class="rl_li_perFee">人均价格：{%$shop.perFee%}元</span>                    </p>                    <p class="fixformargin_price">                        <span class="rl_li_startPrice">起送费：{%$shop.startPrice%}元</span>						<span class="rl_li_serviceFee">							送餐费：{%$shop.startPrice%}元						</span>                    </p>                    <p>                        <span class="rl_li_takeoutTime">送餐时间：{%$shop.takeoutTime%}</span>                    </p>                    {%if $shop.dishCount == 0 %}                    <p class="rl_li_nodish">暂无菜单，直接电话吧 ^_^</p>                    {%/if%}                    {%if ($data.platform == 'android')%}						<a href="javascript:void(0)" class="tj rl_li_tel" onclick="popupDialog('{%$shop.telphone%}')">					{%else%}						<a href="tel:{%$shop.telphone%}" class="tj rl_li_tel">	                {%/if%}                        <em></em>                        {%if $shop.telphone == "0"%}                            暂无电话                        {%else%}                            {%if $data.platform == "ipad"%}                                {%$shop.telphone%}                            {%else%}                                一键订餐                            {%/if%}                        {%/if%}                    </a>					<span class="rl_t_icon rl_mk" style="background-position:0px {%$shop@index*-31%}px">					</span>					<span class="rl_opt rl_li_opt">						<em class="rl_go_dtl" style="display: block;"></em>					</span>                </a>            </li>            {%/foreach%}        </ul>        <div class="change_page" id="change_page_toolbar" style="display:block">            <span class="page_btn {%if ($data.pageNum == 1)%}unclick{%/if%}" data-type="pre" jsaction="jump" data-href="{%if isset($data.prev)%}{%$data.prev%}{%else%}javascript:void(0);{%/if%}">上一页</span>            <span class="page_btn {%if ($data.pageNum == $data.pageTotal)%}unclick{%/if%}" jsaction="jump" data-type="next" data-href="{%if isset($data.next)%}{%$data.next%}{%else%}javascript:void(0);{%/if%}">下一页</span>        </div>    </div></div>{%script%}    $('.tj').click(function(){        var stat = require('common:widget/stat/stat.js');        var name = $('.rl_li_title').text();        stat.addStat(STAT_CODE.PLACE_TAKEOUT_LIST_TELPHONE, {'name':name});    });{%/script%}{%if $data.platform == 'android'%}    {%script%}        window.popupDialog = function(telphone){        	var util = require('common:static/js/util.js');        	util.TelBox.showTb(telphone);        }    {%/script%}{%/if%}{%script%}	require("place:widget/takeoutlist/takeoutlist.js").bind();{%/script%}